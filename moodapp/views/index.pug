html
	head
		title Machine Learning App
		meta(charset='utf-8')
		link(type='text/css', rel='stylesheet', href='/css/style.css')
		link(href='http://fonts.googleapis.com/icon?family=Material+Icons', rel='stylesheet')
		link(type='text/css', rel='stylesheet', href='/css/materialize.min.css', media='screen,projection')
		meta(name='viewport', content='width=device-width, initial-scale=1.0')
	body
		nav
			.nav-wrapper
				a.brand-logo(href='#') Logo
				ul#nav-mobile.right.hide-on-med-and-down
					li
						a(href='sass.html') Sass
					li
						a(href='badges.html') Components
					li
						a(href='collapsible.html') JavaScript
			.row
				form.col.s12(autocomplete="off")
					.row
						.input-field.col.s12
							input#input_text(type='text')
							label(for='input_text' name='query') Text
							p
								input#positive(name='mood', type='radio')
								label(for='positive') Positive
							p
								input#negative(name='mood', type='radio')
								label(for='negative') Negative
							button.btn.waves-effect.waves-light(type='submit')
								| Submit
								


		script(type='text/javascript', src='/js/jquery-3.1.1.min.js')
		script(type='text/javascript', src='/js/materialize.min.js')
		script(type='text/javascript', src='/js/main.js')
			


